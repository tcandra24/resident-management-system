generator client {
  provider = "prisma-client"
  output   = "../lib/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Resident {
  id          String   @id @default(uuid())
  name        String
  description String
  user_id     String
  created_at  DateTime @default(now())
  updated_at  DateTime @updatedAt

  @@map("residents")
}
